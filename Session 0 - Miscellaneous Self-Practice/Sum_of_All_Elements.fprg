    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="i, n, sum" type="Integer" array="False" size=""/>
            <assign variable="sum" expression="0"/>
            <output expression="&quot;Enter the number of elements to be entered in the Array:&quot;" newline="True"/>
            <input variable="n"/>
            <declare name="arr" type="Integer" array="True" size="n"/>
            <for variable="i" start="0" end="n-1" direction="inc" step="1">
                <output expression="&quot;Element &quot; &amp;i&amp; &quot; in the Array is: &quot;" newline="True"/>
                <input variable="arr[i]"/>
            </for>
            <for variable="i" start="0" end="n-1" direction="inc" step="1">
                <assign variable="sum" expression="sum+arr[i]"/>
            </for>
            <output expression="&quot;Sum of all elements stored in the array is : &quot; &amp;sum" newline="True"/>
        </body>
    </function>
